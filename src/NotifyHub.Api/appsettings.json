{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "ConnectionStrings": {
    "NotifyHub": "Server=(local);Database=NotifyHub; Trusted_Connection=True;TrustServerCertificate=True; Integrated security=false"
  },
  "CorsAllowedOrigins": "http://localhost:5500",
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "your-super-secret-key-with-at-least-32-characters",
    "Issuer": "http://localhost:5500",
    "Audience": "http://localhost:5500"
  }
}